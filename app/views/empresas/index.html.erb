<p id="notice"><%= notice %></p>

<h1>Listing Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Titulo1</th>
      <th>Titulo2</th>
      <th>Texto1</th>
      <th>Texto2</th>
      <th>Imagen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.titulo1 %></td>
        <td><%= empresa.titulo2 %></td>
        <td><%= empresa.texto1 %></td>
        <td><%= empresa.texto2 %></td>
         <td><%= image_tag empresa.imagen.url(:small) %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
