<p id="notice"><%= notice %></p>

<h1>Inicio</h1>
<a href="/administrador" class="btn btn-danger">Atr√°s</a>
<table class="table table-hover">
  <thead>
    <tr>
      <th>
        Titulo
      </th>
      <th>Primer texto</th>
      <th>Segundo texto</th>
      <th>Imagen 1</th>
      <th>Imagen 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inicios.each do |inicio| %>
      <tr>
        <td><%= inicio.titulo %></td>
        <td><%= inicio.texto1 %></td>
        <td><%= inicio.texto2 %></td>
        <td><%= image_tag inicio.imagen1.url(:small) %></td>
        <td><%= image_tag inicio.imagen2.url(:small) %></td>
        <td><%= link_to 'Editar', edit_inicio_path(inicio), :class => 'btn btn-primary' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
