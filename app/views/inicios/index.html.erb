<p id="notice"><%= notice %></p>

<h1>Listing Inicios</h1>

<table>
  <thead>
    <tr>
      <th>Texto1</th>
      <th>Texto2</th>
      <th>Imagen1</th>
      <th>Imagen2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inicios.each do |inicio| %>
      <tr>
        <td><%= inicio.texto1 %></td>
        <td><%= inicio.texto2 %></td>
        <td><%= image_tag inicio.imagen1.url(:small) %></td>
        <td><%= image_tag inicio.imagen2.url(:small) %></td>
        <td><%= link_to 'Show', inicio %></td>
        <td><%= link_to 'Edit', edit_inicio_path(inicio) %></td>
        <td><%= link_to 'Destroy', inicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inicio', new_inicio_path %>
